{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tt2",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "if_statement"
        },
        {
          "type": "SYMBOL",
          "name": "for_statement"
        },
        {
          "type": "SYMBOL",
          "name": "block_statement"
        },
        {
          "type": "SYMBOL",
          "name": "variable_reference"
        },
        {
          "type": "SYMBOL",
          "name": "template_tag"
        },
        {
          "type": "SYMBOL",
          "name": "other_text"
        }
      ]
    },
    "if_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "IF"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "for_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "FOR"
        },
        {
          "type": "FIELD",
          "name": "item",
          "content": {
            "type": "SYMBOL",
            "name": "variable_reference"
          }
        },
        {
          "type": "STRING",
          "value": "IN"
        },
        {
          "type": "FIELD",
          "name": "list",
          "content": {
            "type": "SYMBOL",
            "name": "expression"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "block_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "BLOCK"
        },
        {
          "type": "FIELD",
          "name": "block_name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "variable_reference": {
      "type": "PATTERN",
      "value": "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "expression": {
      "type": "PATTERN",
      "value": "[^;]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "template_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "[%-"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "-%]"
        }
      ]
    },
    "other_text": {
      "type": "PATTERN",
      "value": "[^\\[\\]]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}